    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    Variables_BuiltIn:
        - match: |-
            (?x)
            \b
            (
              arg[0-9]*
            | numargs
            )
            (?=  \s  |  \@  |  \$  |  \)  |  \]  |  \;  )
          scope: variable.language.cfg


