%YAML 1.2
---
name: CubeScript
file_extensions: cfg
scope: source.cfg

contexts:
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    main:
        #  Order of includes is important.
        - include: Comments
        - include: Keywords
        - include: Loops__Tesseract
        - include: Loops

        - include: Shaders

        - include: Variables_BuiltIn

        - include: Commands_BuiltIn__Tesseract_UI       #  Put these first to avoid conflicts with similarly
        - include: Commands_BuiltIn__Tesseract_Model    #  named commands inside "Commands_BuiltIn__Sauerbraten".
        - include: Commands_BuiltIn__Tesseract          #
        - include: Commands_BuiltIn__Sauerbraten_GUI
        - include: Commands_BuiltIn__Sauerbraten_Model
        - include: Commands_BuiltIn__Sauerbraten
        - include: Commands_BuiltIn__RPG

        - include: Aliases_Common__Sauerbraten
        - include: Declaration_Alias

        - include: Section_Brackets
        - include: Section_Parentheses
        - include: Section_Strays

        - include: Strings
        - include: StringSubstitution   #  These can appear outside of quoted-strings.
       #- include: TextureCommands      #  These can appear outside of quoted-strings.

        - include: Numbers_Float
        - include: Numbers_Hex
        - include: Numbers_Integer

        - include: Operators_Arithmetic
        - include: Operators_Bitwise
        - include: Operators_Logical
        - include: Operators_Word
        - include: Operators_Assignment
       #- include: Operators_Indiscriminate
       #- include: Operators_Indiscriminate_Word

        - include: AliasEvaluation

        - include: Punctuation
        - include: Wildcard

        - include: Labels_Entity__Sauerbraten
        - include: Labels_Input__Sauerbraten
        - include: Labels_Input__Tesseract
        - include: Labels_Material__Sauerbraten
        - include: Labels_TextureSubSlot
        - include: Labels_Weapon__Sauerbraten

        - include: Aliases_User


    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    #   ALL Aliases are strings.  The strings are parsed and typed upon usage.
    #       Example:  (+f $VAR $VAR)    Since this is a float operation, it will attempt to parse the VAR alias-strings as a float.
    #                                   And, unfortunately, if it can't parse to a float, it will fail silently.  (perhaps there is a setting that can show these errors...)
    #       Example:  (at $VAR 2)       It will attempt to parse VAR as a list and return the contents of the 2nd index. (index here is ZeroInclusive)
    #                                   Whitespace is treated as a delimiter.
    #                                   Containers can be used to have list items with spaces:  [ "a a" "b b" ]
    #
    #   Alias Naming:
    #       Valid characters:
    #           ~ ` ! # % ^ & * _ - = + { }
    #           \ | : ' , < . > ? / @ $ ( [
    #           A-Z   a-z   0-9
    #
    #           Note: Using these chars in aliases is stupid, even if supported.
    #                       | \@ | \$ | \( | \[
    #
    #       Cannot begin with:
    #           0-9  @  $  (  [
    #
    #           Note: Aliases can be named with quoted strings ( "@asdf" = 123 ).
    #                 But this is stupid.
    #
    #       Cannot begin with a number:
    #           [0-9] must be proceeded by [A-Za-z] at some point.
    #
    #======================================================================================================================================
    Declaration_Alias:
        - match: |-
            (?x)
            \b(
              alias
            | local
            )\b
          scope: storage.type.cfg


    #======================================================================================================================================
    AliasEvaluation:
        - match: (\@|\$)
          scope: keyword.operator.cfg


    #======================================================================================================================================
    Aliases_User:
        #  ( Bounds Left )
        #  ( First Character )
        #  ( Characters )
        #  ( Bounds Right )
        - match: |-
            (?x)
            (?<=
                ^ | \s | \@ | \$ | \( | \[ | \) | \] | \;
            )
            (
                (
                    \~ | \` | \! | \# | \% | \^ | \& | \* | \_ | \- | \= | \+ | \{ | \} | \\ | \| | \: | \' | \, | \< | \. | \> | \? | \/ | [A-Za-z]
                ){1}
                (
                    \~ | \` | \! | \# | \% | \^ | \& | \* | \_ | \- | \= | \+ | \{ | \} | \\ | \| | \: | \' | \, | \< | \. | \> | \? | \/ | [A-Za-z0-9]
                )*
            )
            (?=
                \s | \@ | \$ | \) | \] | \;
            )
          scope: variable.other.readwrite.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    Comments:
        - match: (\/\/)
          scope: comment.line.cfg
          push:
            - meta_scope: comment.line.cfg
            - match: (\n)
              pop: true


    ###########################################################################################################################################
    ###########################################################################################################################################
    Keywords:
        - match: |-
            (?x)
            \b(
              at
            | append
            | appendword
            | case[fs]?
            | cond
            | do
            | doargs
            | escape
            | exec
            | if
            | set
            | unescape
            )\s
          scope: keyword.control.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Loops:
        - match: |-
            (?x)
            \b(
              loop
            | while
            | entloop
            | loopconcat
            | loopconcatword
            | loopfiles
            | looplist
            | looplistconcat
            | looplistconcatword
            | loopsublist
            | loopwhile
            )\s
          scope: keyword.control.cfg


    #======================================================================================================================================
    Loops__Tesseract:
        - match: |-
            (?x)
            \b(
              loop( \* | \+ | \+\* )?
            | loopconcat( \* | \+ | \+\* )?
            | loopconcatword( \* | \+ | \+\* )?
            | looplist[23]
            | loopscoreboard
            | looptexmru
            | loopwhile( \* | \+ | \+\* )?
            )\s
          scope: keyword.control.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Numbers_Float:
        - match: |-
            (?x)
            (
                (
                    (
                        (?<![a-zA-Z\d])
                        \d*
                    )
                |
                    (
                        (?<![a-zA-Z\d])
                        \-
                        \d*
                    )
                )
                (
                    (
                        (?<![a-zA-Z\d])
                        \-
                        \d{1}
                        \.{1}
                    )
                |
                    (
                        (?<![\d])
                        \-
                        \.{1}
                        \d{1}
                    )
                |
                    (
                        \d{1}
                        \.{1}
                    )
                |
                    (
                        \.{1}
                        \d{1}
                    )
                ){1}
                \d*
            )
          scope: constant.numeric.float.cfg


    #======================================================================================================================================
    Numbers_Hex:
        - match: |-
            (?x)
            \b(
                (0x){1}
                [0-9a-fA-F]+
            )\b
          scope: constant.numeric.integer.hexadecimal.cfg


    #======================================================================================================================================
    Numbers_Integer:
        - match: |-
            (?x)
            (
                (
                    \b
                    (
                        (?! [a-zA-Z] )
                        \d*
                    )
                    \b
                )
            |
                (
                    (?<! [a-zA-Z\d] )
                    -
                    \d{1}
                    \d*
                )
            )
          scope: constant.numeric.integer.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Operators_Arithmetic:
        - match: |-
            (?x)
            (?<= \( | \[ | \(\s | \[\s )
            (
              \-f
            | \+f
            | \*f
            | \-
            | \+
            | \*
            )
            (?= \s )
          scope: keyword.operator.arithmetic.cfg


    #======================================================================================================================================
    Operators_Bitwise:
        - match: |-
            (?x)
            (?<= ^ | \s | \( | \[ )
            (
              \<\<
            | \>\>
            | \^\~
            | \&\&
            | \&\~
            | \|\|
            | \|\~
            | \!
            | \~
            | \^
            | \&
            | \|
            )
            (?= \s )
          scope: keyword.operator.bitwise.cfg


    #======================================================================================================================================
    Operators_Logical:
        - match: |-
            (?x)
            (?<= \( | \[ | \(\s | \[\s )
            (
              \!\=f
            | \!\=s
            | \<\=f
            | \<\=s
            | \>\=f
            | \>\=s
            | \=f
            | \=s
            | \<f
            | \<s
            | \>f
            | \>s
            | \!\=
            | \<\=
            | \>\=
            | \=
            | \<
            | \>
            | \?
            )
            (?= \s )
          scope: keyword.operator.logical.cfg


    #======================================================================================================================================
    Operators_Assignment:
        - match: |-
            (?x)
            (?<= \s )
            ( \= )
            (?=  \s )
          scope: keyword.operator.assignment.cfg


    #======================================================================================================================================
    Operators_Word:
        - match: |-
            (?x)
            (?<= \( | \[ | \(\s | \[\s )
            (
              absf
            | abs
            | acos
            | asin
            | atan2
            | atan
            | ceil
            | cos
            | divf
            | div
            | exp
            | floor
            | loge
            | log2
            | log10
            | maxf
            | max
            | minf
            | min
            | modf
            | mod
            | pow
            | round
            | sin
            | sqrt
            | tan
            )
            (?= \s )
          scope: keyword.operator.word.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Operators_Indiscriminate:
        - match: |-
            (?x)
            (
              \!
            | \&
            | \*
            | \+
            | \-
            | \<
            | \=
            | \>
            | \?
            | \^
            | \|
            | \~
            )+
            [fs]?
          scope: keyword.operator.cfg


    #======================================================================================================================================
    Operators_Indiscriminate_Word:
        - match: |-
            (?x)
            (
              absf
            | abs
            | acos
            | asin
            | atan2
            | atan
            | ceil
            | cos
            | divf
            | div
            | exp
            | floor
            | loge
            | log2
            | log10
            | maxf
            | max
            | minf
            | min
            | modf
            | mod
            | pow
            | round
            | sin
            | sqrt
            | tan
            )
          scope: keyword.operator.word.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Punctuation:
        - match: \;
          scope: punctuation.terminator.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Section_Brackets:
        - match: \[
          scope: punctuation.section.brackets.begin.cfg
          push:
            - meta_scope: meta.brackets.cfg

            - match: \]
              scope: punctuation.section.brackets.end.cfg
              pop: true

            - include: main


    #======================================================================================================================================
    Section_Parentheses:
        - match: \(
          scope: punctuation.section.parens.begin.cfg
          push:
            - meta_scope: meta.parens.cfg

            - match: \)
              scope: punctuation.section.parens.end.cfg
              pop: true

            - include: main


    #======================================================================================================================================
    Section_Strays:
        - match: \]|\)
          scope: invalid.illegal.stray.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Strings:
        - match: \"(?=.*\")
          scope: punctuation.definition.string.begin.cfg
          push:
            - meta_scope: string.quoted.double.cfg

            - include: StringSubstitution
            - include: TextureCommands

            #  Escape Characters:
            - match: |-
                (?x)
                (
                  ( \^  f [\~0-9sr]{1}  )
                | ( \^  n )
                | ( \^  t )
                | ( \^ \" )
                | ( \^ \^ )
                )
              scope: constant.character.escape.cfg

            #  Invalid Escape Characters:
            - match: (\^.)
              scope: invalid.illegal.character.cfg

            #  END--Strings
            - match: \"|\n
              scope: punctuation.definition.string.end.cfg
              pop: true


    #======================================================================================================================================
    StringSubstitution:
        - match: \%[0-9]+
          scope: constant.other.placeholder.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    TextureCommands:
        - match: \<(?=.*\>)(?!\>)
          scope: punctuation.section.begin.cfg
          push:
            - meta_scope: meta.generic.cfg

            - include: Numbers_Float
            - include: Numbers_Hex
            - include: Numbers_Integer

            #  The Commands:
            - match: |-
                (?x)
                \b(
                  agrad
                | blend
                | blur
                | colorify
                | colormask
                | compress
                | crop
                | dds
                | dup
                | grey
                | mad
                | mirror
                | mix
                | nocompress
                | normal
                | noswizzle
                | offset
                | premul
                | reorient
                | rotate
                | thumbnail
                ){1}\b
              scope: keyword.operator.function.cfg   # keyword.operator.word.cfg  support.function.cfg  entity.name.function.cfg

            #  Separators:
            - match: (\:|\,|\/)
              scope: punctuation.separator.cfg

            #  END--TextureCommands
            - match: (\>|(?=\"))
              scope: punctuation.section.end.cfg
              pop: true


    ###########################################################################################################################################
    ###########################################################################################################################################
    Shaders:
        - match: (?<=^shader|\sshader|^variantshader|\svariantshader|^lazyshader|\slazyshader)
          push:
            #  Cover the gap between "shader" and "[".
            - include: AliasEvaluation
            - include: Numbers_Float
            - include: Numbers_Hex
            - include: Numbers_Integer
            - include: Operators_Arithmetic
            - include: Operators_Bitwise
            - include: Operators_Logical
            - include: Operators_Assignment
            - include: Operators_Word
            - include: Variables_BuiltIn
            - include: Punctuation
            - include: Strings
            - include: Aliases_User

            #  BEGIN--GLSL
            - match: (?<=\[)(?=\s*\n)
              push: Packages/GLSL/GLSL.sublime-syntax
              with_prototype:
                - include: AliasEvaluation

                #  This is typically the separation between Vertex & Fragment sections.
                - match: (^\s*\]\s*\[)(?=\s*\n)
                  scope: punctuation.section.cfg  # constant.other.placeholder.cfg #

                #  END--GLSL
                - match: (?=^\s*\])(?!.*\[) #(?=\s*\n)
                  pop: true

            #  END--Shaders
            - match: (?<=\])|\n
              pop: true

            - match: \[|\]|\(|\)
              scope: punctuation.section.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Wildcard:
        - match: |-
            (?x)
            (?<= \s )
               ( \* )
            (?=  \s | \) | \] | \; )
          scope: wildcard.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
    ###########################################################################################################################################
