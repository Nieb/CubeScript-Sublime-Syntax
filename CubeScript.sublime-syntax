            | SCROLLOCK
            | SEMICOLON
            | SLASH
            | SPACE
            | SYSREQ
            | TAB
            | UNDERSCORE
            | UP
            )
            (?= \s)
          scope: constant.language.cfg


    #======================================================================================================================================
    Labels_Input__Tesseract:
        - match: |-
            (?x)
            (?<= \s)
            (
              MOUSELEFT
            | MOUSEMIDDLE
            | MOUSERIGHT
            | MOUSEWHEELUP
            | MOUSEWHEELDOWN
