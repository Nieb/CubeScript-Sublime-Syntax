            | yellowarmour
            )
            (?= \s | \@ | \$ | \) | \] | \; )
          scope: constant.language.cfg


    ###########################################################################################################################################
    ###########################################################################################################################################
    Labels_Input__Sauerbraten:
        - match: |-
            (?x)
            (?<= \s)
            (
              MOUSE([1-9]|1[0-2]){1}
            | [0-9]{1}
            | [A-Z]{1}
            | F([1-9]|1[0-2]){1}
            | AMPERSAND
            | ASTERISK
            | AT
            | BACKQUOTE
            | BACKSLASH
            | BACKSPACE
            | CAPSLOCK
            | CARET
            | COLON
            | COMMA
            | COMPOSE
            | DELETE
            | DOLLAR
            | DOWN
            | END
            | EQUALS
            | ESCAPE
            | EXCLAIM
            | GREATER
            | HASH
            | HELP
            | HOME
            | INSERT
            | KP[0-9]{1}
            | KP_DIVIDE
            | KP_ENTER
            | KP_EQUALS
            | KP_MINUS
            | KP_MULTIPLY
            | KP_PERIOD
            | KP_PLUS
            | LALT
            | LCTRL
            | LEFT
            | LEFTBRACKET
            | LEFTPAREN
            | LESS
            | LMETA
            | LSHIFT
            | MENU
            | MINUS
            | NUMLOCK
